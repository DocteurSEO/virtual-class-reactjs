er] = useRecoilState(atomEditCurrentUser);\n\n  useEffect(() => {\n    setActive(statut);\n  }, [statut]);\n\n  /** @function closeMenu close the menu   */\n  function closeMenu() {\n    onMenuClick(false);\n  }\n\n  function addUser() {\n    setAnimation({\n      header: \"header\",\n      cards: \"container down\",\n      form: \"form rLeft \",\n      headerTitle: \"Ajouter un élèves \",\n    });\n    setUser({\n      name: \"\",\n      info: \"\",\n      picture: \"\",\n      id: \"\",\n    });\n    closeMenu();\n  }\n\n  return [active, closeMenu, addUser];\n};\n","/home/doc/virtual-class-reactjs/src/components/TopBar/TopBar.jsx",[],"/home/doc/virtual-class-reactjs/src/components/TopBar/TopBarLogic.js",[],"/home/doc/virtual-class-reactjs/src/components/ui/Header/Header.jsx",["110"],"import { useState, useMemo } from \"react\";\nimport PropTypes from \"prop-types\";\nimport { useRecoilValue } from \"recoil\";\n\nimport \"./Header.scss\";\nimport { TopBar } from \"../../TopBar/TopBar\";\nimport { Logo } from \"../../Logo/Logo\";\nimport { Label } from \"../../Label/Label\";\nimport { Menu } from \"../../Menu/Menu\";\n\nimport { atomAnimation } from \"../../../atoms/atomAnimation\";\n\n/**\n *\n * header component : display header with text animation.\n *\n * ###Usage\n *\n * ```jsx\n *\n *<Header text=\"I'm the title ^^ \" />\n *\n * ```\n */\n\nexport const Header = ({ text, color }) => {\n  const [activeMenu, setActiveMenu] = useState(false);\n\n  const animation = useRecoilValue(atomAnimation);\n\n  const displayMenu = useMemo(() => {\n    return (etat) => {\n      setActiveMenu(etat);\n    };\n  }, [activeMenu]);\n\n  return (\n    <div className={animation.header}>\n      <Menu statut={activeMenu} onMenuClick={displayMenu} />\n      <TopBar color={color} />\n      <div class=\"top-container\">\n        <div className=\"r-block top-center-container\">\n          <div className=\"inner-container top-text-container\">\n            <Logo />\n            <p>{text}</p>\n          </div>\n          <Label onLabelClick={displayMenu} />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nHeader.propTypes = {\n  text: PropTypes.string,\n};\n\nHeader.defaultProps = {\n  text: \"...\",\n};\n","/home/doc/virtual-class-reactjs/src/components/TopBar/TopBar.stories.js",[],"/home/doc/virtual-class-reactjs/src/components/ui/Header/Header.stories.js",[],"/home/doc/virtual-class-reactjs/src/components/Cards/Card.jsx",["111"],"import PropTypes from \"prop-types\";\n\nimport perso from \"./img/ahmed.jpg\";\nimport { UseCard } from \"./CardLogic\";\n\n/**\n *\n * @param {object} userInfo props storing the user's information\n * \n * * ```jsx\n *\n *<Card userInfo={ \n   name: \"Ahmed Seneina\",\n    info: \"Developer FullStack Javascript\",\n    picture: perso,\n  } />\n *\n * ```\n */\n\nexport const Card = ({ userInfo, styles, id }) => {\n  const { name, info, picture } = userInfo;\n  const [editUser] = UseCard();\n\n  return (\n    <div\n      class=\"at-column\"\n      style={styles}\n      onClick={() => editUser(id, userInfo)}\n    >\n      <div class=\"at-user\">\n        <div class=\"at-user__avatar\">\n          <img src={picture} alt={\"Erreur, image non disponible \"} />;\n        </div>\n        <div class=\"at-user__name\">{name && name.substr(0, 19)}</div>\n        <div class=\"at-user__title\">{info && info.substr(0, 32)}</div>\n        <ul class=\"at-social\">\n          <li class=\"at-social__item\"></li>\n        </ul>\n      </div>\n    </div>\n  );\n};\nCard.propTypes = {\n  userInfo: PropTypes.objectOf(PropTypes.string),\n};\n\nCard.defaultProps = {\n  userInfo: {\n    name: \"Ahmed Seneina\",\n    info: \"Developer FullStack Javascript\",\n    picture: perso,\n  },\n};\n","/home/doc/virtual-class-reactjs/src/components/Cards/Cards.jsx",[],"/home/doc/virtual-class-reactjs/src/components/Cards/Cards.stories.js",[],"/home/doc/virtual-class-reactjs/src/atoms/atomAnimation.js",[],"/home/doc/virtual-class-reactjs/src/components/Form/Form.jsx",[],"/home/doc/virtual-class-reactjs/src/components/Button/Button.jsx",[],"/home/doc/virtual-class-reactjs/src/components/Form/FormLogic.js",["112"],"/home/doc/virtual-class-reactjs/src/firebase/index.js",[],"/home/doc/virtual-class-reactjs/src/components/Cards/CardLogic.js",["113"],"import { useRecoilState } from \"recoil\";\n\nimport { atomAnimation, atomEditCurrentUser } from \"../../atoms/atomAnimation\";\n\n/**\n * Custom hook which manages the functions for the component Card\n */\n\nexport const UseCard = () => {\n  const [animation, setAnimation] = useRecoilState(atomAnimation);\n  const [currentUser, setCurrentUser] = useRecoilState(atomEditCurrentUser);\n  function editUser(id, userInfo) {\n    setAnimation({\n      ...animation,\n      header: \"\",\n      cards: \" container down\",\n      form: \"form rLeft\",\n      headerTitle: \"Les modifications sont enregistrées en ligne \",\n    });\n\n    setCurrentUser({ ...userInfo, id });\n  }\n  return [editUser];\n};\n","/home/doc/virtual-class-reactjs/src/components/Button/Button.stories.js",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"114","replacedBy":"118"},{"ruleId":"116","replacedBy":"119"},{"ruleId":"120","severity":1,"message":"121","line":23,"column":10,"nodeType":"122","messageId":"123","endLine":23,"endColumn":19},{"ruleId":"120","severity":1,"message":"124","line":24,"column":10,"nodeType":"122","messageId":"123","endLine":24,"endColumn":14},{"ruleId":"125","severity":1,"message":"126","line":35,"column":6,"nodeType":"127","endLine":35,"endColumn":18,"suggestions":"128"},{"ruleId":"129","severity":1,"message":"130","line":33,"column":11,"nodeType":"131","endLine":33,"endColumn":70},{"ruleId":"125","severity":1,"message":"132","line":50,"column":6,"nodeType":"127","endLine":50,"endColumn":16,"suggestions":"133"},{"ruleId":"120","severity":1,"message":"134","line":11,"column":10,"nodeType":"122","messageId":"123","endLine":11,"endColumn":2